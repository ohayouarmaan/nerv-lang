type customInt : int;
type customIntTwo : customInt;
type customIntThree : customIntTwo;

extern printf(string, int) int;
extern malloc(int) &int;

struct Point {
  x int,
  y int,
}

@main() customIntThree {
  dec validPointer &customInt = returnsPointerToAHeapAllocatedInt();
  dec danglingPointer &customInt = returnsPointerToAStackVariable();
  dec pone Point = {
    x: 4,
    y: 4
  };
  printf("Value of valid pointer: %d\n", *validPointer);
  printf("Value of dangling pointer: %d\n", *danglingPointer);
  return 0;
}

@returnsPointerToAStackVariable() &customIntThree {
  dec a customInt = 4;
  return &a;
}

@returnsPointerToAHeapAllocatedInt(int initialValue) &customInt {
  dec x &int = malloc(4);
  *x = 45;
  return x;
}
